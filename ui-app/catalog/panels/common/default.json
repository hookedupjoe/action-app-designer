{
	"options": {
		"readonly": false,
		"padding": true
	},
	"content": [
		{
			"ctl": "title",
			"name": "dyno-title",
			"size": "large",
			"color": {
				"[computed]": {
					"context": "page.owner.getDefaultColor()"
			 }
			},
			"icon": "box",
			"text": {
				"[computed]": {
					"context": "page.data.demoTitle"
			 }
			}
		},
		{
			"ctl": "button",
			"color": "green",
			"icon": "search",
			"name": "btn-search",
			"onClick": {
				"run": "action",
				"action": "runSearch",
				"params": {
					"q": "Testing"
				}
			}
		},
		{
			"ctl": "title",
			"name": "title",
			"size": "large",
			"color": "blue",
			"icon": "globe",
			"text": "My First Application"
		},
		{
			"ctl": "tabs",
			"name": "apptabs",
			"tabs": [
				{
					"label": "Design",
					"name": "apptabs-design",
					"ctl": "tab",
					"content": [
						{
							"ctl": "tabs",
							"name": "apptabs-design-tabs",
							"tabs": [
								{
									"label": "Pages",
									"name": "apptabs-pages",
									"ctl": "tab",
									"content": [
										{
											"ctl": "panel",
											"controlname": "design/ws/get-pages?appname=app001",
											"name": "pages"
										}
									]
								},
								{
									"label": "Catalog",
									"name": "apptabs-catalog",
									"ctl": "tab",
									"content": [
										{
											"ctl": "pagespot",
											"spotname": "apptabs-catalog",
											"text": "CATALOG GOES HERE"
										}
									]
								}
							]
						}
					]
				},
				{
					"label": "Setup",
					"name": "apptabs-setup",
					"ctl": "tab",
					"content": [
						{
							"ctl": "panel",
							"controlname": "design/ws/panel-app-setup?appname=app001",
							"name": "setupinfo"
						}
					]
				},
				{
					"label": "Preview",
					"name": "apptabs-preview",
					"ctl": "tab",
					"content": [
						{
							"ctl": "pagespot",
							"spotname": "apptabs-preview"
						}
					]
				},
				{
					"label": "Deploy",
					"name": "apptabs-deploy",
					"ctl": "tab",
					"content": [
						{
							"ctl": "pagespot",
							"spotname": "apptabs-deploy"
						}
					]
				}
			]
		}
	]
}